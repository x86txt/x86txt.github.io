@font-face {
	font-family: 'Roboto';
	font-style: normal;
	font-weight: 100;
	src: local('Roboto Thin'), local('Roboto-Thin'), url(http://themes.googleusercontent.com/static/fonts/roboto/v10/vzIUHo9z-oJ4WgkpPOtg1_esZW2xOQ-xsNqO47m55DA.woff) format('woff');
}
article {
	display: block;
}
aside {
	display: block;
}
details {
	display: block;
}
figcaption {
	display: block;
}
figure {
	display: block;
	margin: 0;
}
footer {
	display: block;
}
header {
	display: block;
}
hgroup {
	display: block;
}
main {
	display: block;
}
nav {
	display: block;
}
section {
	display: block;
}
summary {
	display: block;
}
audio {
	display: inline-block;
	&:not([controls]) {
		display: none;
		height: 0;
	}
}
canvas {
	display: inline-block;
}
video {
	display: inline-block;
}
[hidden] {
	display: none;
}
template {
	display: none;
}
html {
	font-family: $font-family_1;
	-ms-text-size-adjust: 100%;
	-webkit-text-size-adjust: 100%;
	margin: auto;
	padding-right: 1em;
	padding-left: 1em;
	max-width: 64em;
	color: $color_2;
	input[type="button"] {
		-webkit-appearance: button;
		cursor: pointer;
	}
	input[disabled] {
		cursor: default;
	}
}
body {
	margin: 0;
	margin: auto;
	padding-right: 1em;
	padding-left: 1em;
	max-width: 64em;
	color: $color_2;
	font: 13.34pxHelvetica;
	-webkit-font-smoothing: subpixel-antialiased;
	line-height: 1.4;
	padding: 3px;
	background: #fff;
	border-radius: 3px;
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	background-color: $background-color_2;
	padding: 30px;
	margin: 15px;
	font-size: 14px;
	line-height: 1.6;
	>* {
		&:first-child {
			margin-top: 0 !important;
		}
		&:last-child {
			margin-bottom: 0 !important;
		}
	}
	>h2 {
		&:first-child {
			margin-top: 0;
			padding-top: 0;
		}
	}
	>h1 {
		&:first-child {
			margin-top: 0;
			padding-top: 0;
		}
		&:first-child+h2 {
			margin-top: 0;
			padding-top: 0;
		}
	}
	>h3 {
		&:first-child {
			margin-top: 0;
			padding-top: 0;
		}
	}
	>h4 {
		&:first-child {
			margin-top: 0;
			padding-top: 0;
		}
	}
	>h5 {
		&:first-child {
			margin-top: 0;
			padding-top: 0;
		}
	}
	>h6 {
		&:first-child {
			margin-top: 0;
			padding-top: 0;
		}
	}
}
a {
	background: transparent;
	color: $color_3;
	text-decoration: none;
	&:focus {
		outline: thin dotted;
	}
	&:active {
		outline: 0;
	}
	&:hover {
		outline: 0;
	}
	&:first-child {
		h1 {
			margin-top: 0;
			padding-top: 0;
		}
		h2 {
			margin-top: 0;
			padding-top: 0;
		}
		h3 {
			margin-top: 0;
			padding-top: 0;
		}
		h4 {
			margin-top: 0;
			padding-top: 0;
		}
		h5 {
			margin-top: 0;
			padding-top: 0;
		}
		h6 {
			margin-top: 0;
			padding-top: 0;
		}
	}
}
h1 {
	font-size: 2em;
	margin: 0.67em 0;
	margin: 20px 0 10px;
	padding: 0;
	font-weight: bold;
	-webkit-font-smoothing: subpixel-antialiased;
	cursor: text;
	font-size: 37px;
	color: $color_1;
	text-align: center;
}
abbr[title] {
	border-bottom: 1px dotted;
}
b {
	font-weight: bold;
}
strong {
	font-weight: bold;
}
dfn {
	font-style: italic;
}
hr {
	-moz-box-sizing: content-box;
	box-sizing: content-box;
	height: 0;
	background: transparent url(data:image/png;base64,) repeat-x 0 0;
	border: 0 none;
	color: $color_7;
	height: 4px;
	padding: 0;
}
mark {
	background: #ff0;
	color: $color_1;
}
code {
	font-family: $font-family_2;
	font-size: 1em;
	margin: 0 2px;
	padding: 0 5px;
	white-space: nowrap;
	border: 1px solid #eaeaea;
	background-color: $background-color_4;
	border-radius: 3px;
	font-family: $font-family_4;
	font-size: 12px;
	color: $color_5;
}
kbd {
	font-family: $font-family_2;
	font-size: 1em;
}
pre {
	font-family: $font-family_2;
	font-size: 1em;
	white-space: pre-wrap;
	margin: 15px 0;
	background-color: $background-color_4;
	border: 1px solid #ccc;
	font-size: 13px;
	line-height: 19px;
	overflow: auto;
	padding: 6px 10px;
	border-radius: 3px;
	>code {
		margin: 0;
		padding: 0;
		white-space: pre;
		border: 0;
		background: transparent;
	}
	code {
		background-color: $background-color_5;
		border: 0;
	}
	tt {
		background-color: $background-color_5;
		border: 0;
	}
}
samp {
	font-family: $font-family_2;
	font-size: 1em;
}
q {
	quotes: "\201C""\201D""\2018""\2019";
}
small {
	font-size: 80%;
}
sub {
	font-size: 75%;
	line-height: 0;
	position: relative;
	vertical-align: baseline;
	bottom: -0.25em;
	font-size: 1.4ex;
	height: 0;
	line-height: 1;
	vertical-align: super;
	position: relative;
	vertical-align: sub;
	top: -1px;
}
sup {
	font-size: 75%;
	line-height: 0;
	position: relative;
	vertical-align: baseline;
	top: -0.5em;
	font-size: 1.4ex;
	height: 0;
	line-height: 1;
	vertical-align: super;
	position: relative;
}
img {
	border: 0;
	max-width: 100%;
}
svg {
	&:not(:root) {
		overflow: hidden;
	}
}
fieldset {
	border: 1px solid #c0c0c0;
	margin: 0 2px;
	padding: 0.35em 0.625em 0.75em;
}
legend {
	border: 0;
	padding: 0;
}
button {
	font-family: $font-family_3;
	font-size: 100%;
	margin: 0;
	line-height: normal;
	text-transform: none;
	-webkit-appearance: button;
	cursor: pointer;
	&::-moz-focus-inner {
		border: 0;
		padding: 0;
	}
}
input {
	font-family: $font-family_3;
	font-size: 100%;
	margin: 0;
	line-height: normal;
	&::-moz-focus-inner {
		border: 0;
		padding: 0;
	}
}
select {
	font-family: $font-family_3;
	font-size: 100%;
	margin: 0;
	text-transform: none;
}
textarea {
	font-family: $font-family_3;
	font-size: 100%;
	margin: 0;
	overflow: auto;
	vertical-align: top;
}
input[type="reset"] {
	-webkit-appearance: button;
	cursor: pointer;
}
input[type="submit"] {
	-webkit-appearance: button;
	cursor: pointer;
}
button[disabled] {
	cursor: default;
}
input[type="checkbox"] {
	box-sizing: border-box;
	padding: 0;
}
input[type="radio"] {
	box-sizing: border-box;
	padding: 0;
}
input[type="search"] {
	-webkit-appearance: textfield;
	-moz-box-sizing: content-box;
	-webkit-box-sizing: content-box;
	box-sizing: content-box;
	&::-webkit-search-cancel-button {
		-webkit-appearance: none;
	}
	&::-webkit-search-decoration {
		-webkit-appearance: none;
	}
}
table {
	border-collapse: collapse;
	border-spacing: 0;
	margin: 15px 0;
	border-collapse: collapse;
	border-spacing: 0;
	font-size: 100%;
	font: inherit;
	th {
		font-weight: bold;
		border: 1px solid #ccc;
		padding: 6px 13px;
	}
	td {
		border: 1px solid #ccc;
		padding: 6px 13px;
	}
	tr {
		border-top: 1px solid #ccc;
		background-color: $background-color_3;
		&:nth-child(2n) {
			background-color: $background-color_4;
		}
	}
}
.go-top {
	position: fixed;
	bottom: 2em;
	right: 2em;
	text-decoration: none;
	background-color: $background-color_1;
	font-size: 12px;
	padding: 1em;
	display: inline;
}
* {
	&:not('#mkdbuttons') {
		margin: 0;
		padding: 0;
	}
}
p {
	margin: 1em 0;
	margin: 15px 0;
}
h2 {
	margin: 20px 0 10px;
	padding: 0;
	font-weight: bold;
	-webkit-font-smoothing: subpixel-antialiased;
	cursor: text;
	font-size: 24px;
	border-bottom: 1px solid #ccc;
	color: $color_4;
}
h3 {
	margin: 20px 0 10px;
	padding: 0;
	font-weight: bold;
	-webkit-font-smoothing: subpixel-antialiased;
	cursor: text;
	font-size: 18px;
	color: $color_5;
	-webkit-margin-before: 0.01em;
	-webkit-margin-after: 0.01em;
}
h4 {
	margin: 20px 0 10px;
	padding: 0;
	font-weight: bold;
	-webkit-font-smoothing: subpixel-antialiased;
	cursor: text;
	font-size: 16px;
	color: $color_5;
	text-align: center;
	-webkit-margin-before: 0.02em;
	-webkit-margin-after: 0.02em;
}
h5 {
	margin: 20px 0 10px;
	padding: 0;
	font-weight: bold;
	-webkit-font-smoothing: subpixel-antialiased;
	cursor: text;
	font-size: 14px;
	color: $color_5;
	text-align: center;
}
h6 {
	margin: 20px 0 10px;
	padding: 0;
	font-weight: bold;
	-webkit-font-smoothing: subpixel-antialiased;
	cursor: text;
	color: $color_6;
	font-size: 14px;
	text-align: center;
}
blockquote {
	margin: 15px 0;
	border-left: 4px solid #DDD;
	padding: 0 15px;
	color: $color_6;
	> {
		&:first-child {
			margin-top: 0;
		}
		&:last-child {
			margin-bottom: 0;
		}
	}
}
ul {
	padding-left: 30px;
	li {
		> {
			&:first-child {
				margin-top: 0;
			}
		}
	}
}
ol {
	padding-left: 30px;
	li {
		ul {
			&:first-of-type {
				margin-top: 0;
			}
		}
		> {
			&:first-child {
				margin-top: 0;
			}
		}
	}
}
h1+p {
	margin-top: 0;
}
h2+p {
	margin-top: 0;
}
h3+p {
	margin-top: 0;
}
h4+p {
	margin-top: 0;
}
h5+p {
	margin-top: 0;
}
h6+p {
	margin-top: 0;
}
dl {
	padding: 0;
	dt {
		font-size: 14px;
		font-weight: bold;
		font-style: italic;
		padding: 0;
		margin: 15px 0 5px;
		&:first-child {
			padding: 0;
		}
		> {
			&:first-child {
				margin-top: 0;
			}
			&:last-child {
				margin-bottom: 0;
			}
		}
	}
	dd {
		margin: 0 0 15px;
		padding: 0 15px;
		> {
			&:first-child {
				margin-top: 0;
			}
			&:last-child {
				margin-bottom: 0;
			}
		}
	}
}
tt {
	margin: 0 2px;
	padding: 0 5px;
	white-space: nowrap;
	border: 1px solid #eaeaea;
	background-color: $background-color_4;
	border-radius: 3px;
	font-family: $font-family_4;
	font-size: 12px;
	color: $color_5;
}
